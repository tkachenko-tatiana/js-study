    "typescript-eslint-parser": "^22.0.0",
    "webpack": "^4.29.6",
    "webpack-cli": "^3.2.3",
    "webpack-dev-server": "^3.2.1",
    "webpack-hot-middleware": "^2.24.3"
  },
  "engines": {
    "node": "=11.14.0"
  },
  "jest": {
    "testURL": "http://localhost/",
    "preset": "ts-jest",
    "testMatch": [
      "<rootDir>/__tests__/**/*.test.(tsx|ts)"
    ],
    "setupFilesAfterEnv": [
      "<rootDir>/__tests__/setupTests.ts"
    ],
    "moduleDirectories": [
      "node_modules",
      "src"
    ],
    "moduleNameMapper": {
      "\\.(jpg|jpeg|png|gif|eot|otf|svg|ttf|woff|woff2)$": "<rootDir>/__tests__/__mocks__/fileMock.ts",
      "\\.(css|scss)$": "<rootDir>/__tests__/__mocks__/styleMock.ts"
    }
  },
  "version": "0.0.1"
