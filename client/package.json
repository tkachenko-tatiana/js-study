    "tslint-react": "^3.6.0",
    "typescript": "^3.2.2",
    "webpack-merge": "^4.1.3"
  },
  "devDependencies": {
    "@babel/cli": "^7.1.5",
    "@babel/core": "^7.1.6",
    "@babel/plugin-proposal-class-properties": "^7.1.0",
