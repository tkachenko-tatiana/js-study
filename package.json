{
  "main": "build/index.js",
  "name": "js-study",
  "description": "App for study",
  "scripts": {
    "start": "lerna run prod",
    "dev": "lerna run dev --parallel",
    "postinstall": "lerna bootstrap",
    "db:migrate": "lerna run db:migrate --parallel",
    "post-build": "cd build && pwd && sh scripts/production-install.sh",
    "build": "rimraf build/* && lerna run build && node scripts/copy-build-files.js && post-build",
    "test": "lerna run test --parallel"
  },
  "dependencies": {
    "copyfiles": "^2.1.0",
    "lerna": "^3.13.1",
    "rimraf": "^2.6.3",
    "ts-loader": "^5.3.3",
    "tslint": "^5.13.1",
    "tslint-react": "^3.6.0"
  },
  "devDependencies": {
    "@types/jest": "^24.0.11",
    "eslint": "^5.15.1",
    "eslint-config-airbnb": "^17.1.0",
    "eslint-plugin-react": "^7.12.4",
    "husky": "^1.3.1",
    "jest": "^24.3.1",
    "ts-jest": "^24.0.0",
    "tslint-config-airbnb": "^5.11.1",
    "tslint-eslint-rules": "^5.4.0",
    "typescript": "^3.3.3333",
    "typescript-eslint-parser": "^22.0.0",
    "webpack": "^4.29.6"
  },
  "engines": {
    "node": ">=9.2.0"
  },
  "version": "0.0.1"
}
