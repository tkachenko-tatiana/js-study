{
  "name": "js-study-server",
  "version": "0.0.1",
  "main": "build/index.js",
  "description": "App for study",
  "license": "MIT",
  "scripts": {
    "dev": "ts-node-dev index.ts --inspect",
    "db:migrate": "ts-node ./node_modules/.bin/typeorm migration:run",
    "db:migration:generate": "ts-node ./node_modules/.bin/typeorm migration:generate -n name",
    "db:migration:revert": "ts-node ./node_modules/.bin/typeorm migration:revert",
    "postinstall": "npx lerna link",
    "test": "cross-env NODE_ENV=test TS_NODE_TRANSPILE_ONLY=true mocha --opts __tests__/mocha.opts __tests__/**/*.test.ts",
    "test:ci": "cross-env NODE_ENV=test npm run db:migrate && npm run test",
    "build": "tsc --project .",
    "prod": "node index.js",
    "prodTest": "pm2 reload ecosystem.config.js"
  },
  "dependencies": {
    "@types/expect": "^1.20.4",
    "@types/koa-mount": "^3.0.1",
    "@types/supertest": "^2.0.7",
    "cross-env": "^5.2.0",
    "dotenv": "^6.2.0",
    "expect": "^24.3.1",
    "http-status": "^1.3.1",
    "inversify": "^5.0.1",
    "jsonwebtoken": "^8.5.0",
    "koa": "^2.7.0",
    "koa-bodyparser": "^4.2.1",
    "koa-combine-routers": "^4.0.2",
    "koa-jwt": "^3.5.1",
    "koa-mount": "^4.0.0",
    "koa-router": "^7.4.0",
    "koa-static": "^5.0.0",
    "pg": "^7.8.2",
    "pm2": "^3.3.1",
    "reflect-metadata": "^0.1.13",
    "require-all": "^3.0.0",
    "supertest": "^4.0.0",
    "ts-node": "^8.0.3",
    "typeorm": "^0.2.14",
    "typescript": "^3.3.3333",
    "uuid": "^3.3.2"
  },
  "devDependencies": {
    "@types/http-status": "^0.2.30",
    "@types/jsonwebtoken": "^8.3.2",
    "@types/koa": "^2.0.48",
    "@types/koa-bodyparser": "^4.2.2",
    "@types/koa-router": "^7.0.40",
    "@types/koa-static": "^4.0.1",
    "@types/mocha": "^5.2.6",
    "@types/node": "^11.11.0",
    "@types/uuid": "^3.4.4",
    "mocha": "^6.0.2",
    "ts-mocha": "^6.0.0",
    "ts-node-dev": "^1.0.0-pre.32"
  },
  "engines": {
    "node": ">=9.2.0"
  }
}
