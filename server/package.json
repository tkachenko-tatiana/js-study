{
  "name": "js-study-server",
  "version": "0.0.1",
  "main": "build/index.js",
  "description": "App for study",
  "license": "MIT",
  "scripts": {
    "dev": "ts-node-dev index.ts --inspect",
    "db:migrate": "ts-node ./node_modules/.bin/typeorm migration:run",
    "db:migration:generate": "ts-node ./node_modules/.bin/typeorm migration:generate -n name",
    "db:migration:revert": "ts-node ./node_modules/.bin/typeorm migration:revert",
    "postinstall": "npx lerna link"
  },
  "dependencies": {
    "dotenv": "^6.2.0",
    "http-status": "^1.3.1",
    "inversify": "^5.0.1",
    "jsonwebtoken": "^8.4.0",
    "koa": "^2.6.2",
    "koa-bodyparser": "^4.2.1",
    "koa-combine-routers": "^4.0.2",
    "koa-jwt": "^3.5.1",
    "koa-router": "^7.4.0",
    "pg": "^7.7.1",
    "reflect-metadata": "^0.1.12",
    "require-all": "^3.0.0",
    "ts-node": "^7.0.1",
    "typeorm": "^0.2.9",
    "typescript": "^3.2.2",
    "uuid": "^3.3.2"
  },
  "devDependencies": {
    "@types/http-status": "^0.2.30",
    "@types/jsonwebtoken": "^8.3.0",
    "@types/koa": "^2.0.47",
    "@types/koa-bodyparser": "^4.2.1",
    "@types/koa-router": "^7.0.35",
    "@types/node": "^10.12.18",
    "@types/uuid": "^3.4.4",
    "ts-node-dev": "^1.0.0-pre.32"
  },
  "engines": {
    "node": ">=8.11.3"
  }
}
